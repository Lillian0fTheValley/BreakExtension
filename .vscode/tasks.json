{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "npm: compile",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"compile"
			],
			"isBackground": false,
			"presentation": {
				"reveal": "always"
			},
			"problemMatcher": "$tsc"
		},
		{
			"label": "npm: watch",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"watch"
			],
			"isBackground": true,
			"presentation": {
				"reveal": "always"
			},
			"problemMatcher": "$tsc-watch"
		}
	]
}
